<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sf="http://schemas.syncfusion.com/wpf">
    
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/Syncfusion.Themes.Windows11Light.WPF;component/toolbaradv/toolbaradv.xaml"/>
    </ResourceDictionary.MergedDictionaries>
    
    <Style x:Key="DefaultSfToolBarStyle" TargetType="sf:ToolBarAdv" BasedOn="{StaticResource SyncfusionToolBarAdvStyle}">
        <Setter Property="EnableAddRemoveButton" Value="False"/>
        <Setter Property="GripperVisibility" Value="Collapsed"/>
    </Style>
    
</ResourceDictionary>